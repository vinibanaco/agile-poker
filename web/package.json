{
  "name": "web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "react-app-rewired --expose-gc test --colors --logHeapUsage",
    "test:watchAll": "react-app-rewired --expose-gc test --colors --logHeapUsage --watchAll",
    "lint-fix": "eslint --ext js,jsx,ts,tsx --fix .",
    "lint-staged-files": "eslint --fix",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|md)\"",
    "format-staged-files": "prettier --write",
    "prepare-husky": "cd .. && husky install",
    "pre-commit-lint": "lint-staged"
  },
  "dependencies": {
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "typescript": "4.5.4"
  }
}
